
{
  "expo": {
    "name": "Rink Rumble Stats",
    "slug": "rink-rumble-stats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/final_quest_240x240.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/final_quest_240x240.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F8FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.rinkrumblestats",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/final_quest_240x240.png",
        "backgroundColor": "#F5F8FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.rinkrumblestats"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "scheme": "rinkrumblestats",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
